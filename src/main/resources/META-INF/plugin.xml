<idea-plugin>
    <id>com.mclaughlinconnor.ij_inspector</id>
    <name>IJ Inspector</name>
    <vendor>Connor McLaughlin</vendor>

    <depends>com.intellij.modules.platform</depends>

    <resource-bundle>messages.MyBundle</resource-bundle>

    <extensions defaultExtensionNs="com.intellij">
        <appStarter id="completion_inspect" implementation="com.mclaughlinconnor.ij_inspector.application.CompletionStarter"/>
    </extensions>
</idea-plugin>
